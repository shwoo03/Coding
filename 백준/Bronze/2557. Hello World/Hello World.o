section .data  ; msg 라벨에 문자열 저장하고 줄바꿈 문자 저장 후 길이 저장
        msg db "Hello World!", 0xA, 0 ; printf("Hello World!\nNULL");

section .text
        global main
        extern printf

main:
        ; printf()
        mov rdi, msg ; 문자열 입력
        xor rax, rax
        call printf                                                                                                                                                                                                                                     ;return 0                                                                                                               xor rax, rax                                                                                                            ret   